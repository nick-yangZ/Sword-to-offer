#include"ListNode.h"
using namespace std;

//面试题65-不用加减乘除做加法
//通过先(a&b)<<1|a^b得出结果。

int GetSum(int a, int b) {

	return ((a&b) << 1) | (a^b);
}

int main() {
	GetSum(10, 300);
	return 0;
}
